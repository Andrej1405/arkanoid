{
  "env": {
    "browser": true,
    "es2021": true
  },
  
	"extends": [
		"prettier"
	],

  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  
	"rules": {
    "quotes": [
      "error",
      "single"
    ],
    
		"semi": [
      "error",
      "never"
    ]
  }
}
